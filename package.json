{
  "name": "hope-ui-solid",
  "version": "0.0.20",
  "private": false,
  "description": "The hoped component library for your SolidJS applications.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "framework",
    "frontend",
    "components"
  ],
  "homepage": "https://github.com/fabien-ml/hope-ui#readme",
  "bugs": {
    "url": "https://github.com/fabien-ml/hope-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabien-ml/hope-ui.git"
  },
  "license": "MIT",
  "author": "Fabien MARIE-LOUISE (https://github.com/fabien-ml)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:storybook": "build-storybook",
    "commit": "git-cz",
    "dev": "vite",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest",
    "test:unit:coverage": "jest --silent --watchAll=false --coverage",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "csstype": "3.0.10",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@commitlint/cz-commitlint": "16.0.0",
    "@stitches/core": "1.2.6",
    "@storybook/addon-a11y": "6.4.12",
    "@storybook/addon-actions": "6.4.12",
    "@storybook/addon-essentials": "6.4.12",
    "@storybook/addon-links": "6.4.12",
    "@storybook/builder-webpack5": "6.4.12",
    "@storybook/html": "6.4.12",
    "@storybook/manager-webpack5": "6.4.12",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/jest": "^27.4.0",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "17.0.8",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "babel-loader": "8.2.3",
    "babel-preset-solid": "1.3.2",
    "commitizen": "4.2.4",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-solid": "0.4.0",
    "husky": "7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "solid-js": "1.3.3",
    "solid-testing-library": "^0.2.1",
    "sort-package-json": "1.53.1",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.4",
    "vite": "2.7.11",
    "vite-plugin-dts": "0.9.9",
    "vite-plugin-solid": "2.2.2"
  },
  "peerDependencies": {
    "@stitches/core": "^1.2.6",
    "solid-js": "^1.3.3"
  }
}
