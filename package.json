{
  "name": "@hope-ui/solid",
  "version": "0.0.1",
  "private": false,
  "description": "The utopic component library for your SolidJS Applications.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "framework",
    "frontend",
    "components"
  ],
  "homepage": "https://github.com/fabien-ml/hope-ui#readme",
  "bugs": {
    "url": "https://github.com/fabien-ml/hope-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabien-ml/hope-ui.git"
  },
  "license": "MIT",
  "author": "Fabien MARIE-LOUISE (https://github.com/fabien-ml)",
  "sideEffects": [
    "*.scss"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles/": "./dist/styles/"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "commit": "git-cz",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "lodash.merge": "^4.6.2",
    "modern-normalize": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/cz-commitlint": "^15.0.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.47.0",
    "solid-js": "^1.3.1",
    "sort-package-json": "^1.53.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-solid": "^2.2.1"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}
